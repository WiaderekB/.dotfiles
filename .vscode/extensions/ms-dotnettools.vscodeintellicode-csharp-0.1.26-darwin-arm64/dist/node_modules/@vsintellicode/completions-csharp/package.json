{
    "name": "@vsintellicode/completions-csharp",
    "version": "1.0.0-beta.g0a87c30039",
    "description": "IntelliCode Completions for C#",
    "main": "index.js",
    "types": "index.d.ts",
    "publisher": "VisualStudioExptTeam",
    "author": {
        "name": "Microsoft"
    },
    "scripts": {
        "compile": "npm run webpack",
        "eslint": "eslint src --ext ts",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "watch": "tsc -watch -p ./",
        "devpack": "webpack --mode development",
        "webpack": "npm run compile:pythia-compose-shared && webpack --mode production",
        "devpack:green-only": "webpack --mode development",
        "webpack:green-only": "webpack --mode production",
        "compile:pythia-compose-shared": "cd ../pythia-compose-shared && npm i && npm run compile",
        "nerdbank-set": "nbgv-setversion",
        "nerdbank-reset": "nbgv-setversion --reset"
    },
    "dependencies": {
        "nerdbank-gitversioning": "^3.6.65-alpha"
    },
    "devDependencies": {
        "@types/lru-cache": "^5.1.0",
        "@types/node": "^12.20.15",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "copy-webpack-plugin": "^11.0.0",
        "dts-bundle": "^0.7.3",
        "dts-bundle-webpack": "^1.0.2",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "prettier": "^1.19.1",
        "pythia-compose-shared": "file:../pythia-compose-shared/dist",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.0",
        "vscode-nls-dev": "^3.2.6",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1",
        "onnxruntime-node": "^1.13.0"
    },
    "bundledDependencies": [
        "onnxruntime-node",
        "onnxruntime-common"
    ]
}
